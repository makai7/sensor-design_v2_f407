{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "STM32 JLink Debug",
            "type": "cortex-debug",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/V2.2_F407.elf",
            "servertype": "jlink",
            "device": "STM32F407VE",
            "interface": "swd",
            "runToEntryPoint": "main",
            "svdFile": "${workspaceFolder}/.vscode/STM32F407.svd",
            "preLaunchTask": "Build Firmware",
            "showDevDebugOutput": "raw",
            "armToolchainPath": "/usr/bin",
            "gdbPath": "arm-none-eabi-gdb",
            "swoConfig": {
                "enabled": true,
                "cpuFrequency": 168000000,
                "swoFrequency": 2000000,
                "source": "probe",
                "decoders": [
                    {
                        "type": "console",
                        "label": "ITM",
                        "port": 0
                    }
                ]
            }
        },
        {
            "name": "STM32 JLink Attach",
            "type": "cortex-debug",
            "request": "attach",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/V2.2_F407.elf",
            "servertype": "jlink",
            "device": "STM32F407VE",
            "interface": "swd",
            "svdFile": "${workspaceFolder}/.vscode/STM32F407.svd",
            "armToolchainPath": "/usr/bin",
            "gdbPath": "arm-none-eabi-gdb"
        }
    ]
}
